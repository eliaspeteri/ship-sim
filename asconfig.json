{
  "targets": {
    "debug": {
      "outFile": "build/debug.wasm",
      "textFile": "build/debug.wat",
      "sourceMap": true,
      "debug": true,
      "importMemory": true,
      "initialMemory": 16,
      "maximumMemory": 100,
      "runtime": "stub",
      "noTreeShaking": true
    },
    "release": {
      "outFile": "public/wasm/ship_sim.wasm",
      "textFile": "public/wasm/ship_sim.wat",
      "sourceMap": true,
      "optimizeLevel": 3,
      "shrinkLevel": 2,
      "converge": true,
      "importMemory": true,
      "initialMemory": 16,
      "maximumMemory": 100,
      "runtime": "stub",
      "noAssert": true,
      "noTreeShaking": true
    }
  },
  "options": {
    "bindings": "raw",
    "exportRuntime": true,
    "showConfig": true
  }
}
